
#include <iostream>
#include <string>
using namespace std;
int main() {
    string first_name, middle_name, last_name;

    cout << "Enter your first name: ";
    cin >> first_name;

    cout << "Enter your middle name: ";
    cin >> middle_name;

    cout << "Enter your last name: ";
    cin >> last_name;

    cout << "Your initials are: " << first_name[0] << middle_name[0] << last_name[0] << endl;

    return 0;
}
