#include <iostream>
#include <string>

int main() {
    std::string first_name, middle_name, last_name;

    std::cout << "Enter your first name: ";
    std::cin >> first_name;

    std::cout << "Enter your middle name: ";
    std::cin >> middle_name;

    std::cout << "Enter your last name: ";
    std::cin >> last_name;

    std::cout << "Your initials are: " << first_name[0] << middle_name[0] << last_name[0] << std::endl;

    return 0;
}
